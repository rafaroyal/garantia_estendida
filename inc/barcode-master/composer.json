{
    "name": "eumatheusgomes/barcode",
    "description": "A PHP library for barcode creation.",
    "type": "library",
    "keywords": [
        "barcode",
        "2 of 5",
        "interleaved",
        "html"
    ],
    "homepage": "https://github.com/eumatheusgomes/barcode",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Gomes",
            "email": "eu@matheusgomes.com.br",
            "homepage": "https://github.com/eumatheusgomes",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "EuMatheusGomes\\Barcode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EuMatheusGomes\\Barcode\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
